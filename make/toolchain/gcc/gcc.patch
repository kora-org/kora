diff '--color=auto' -uNr gcc-11.2.0/gcc/config/faruos.h gcc-11.2.0.faruos/gcc/config/faruos.h
--- gcc-11.2.0/gcc/config/faruos.h	1970-01-01 07:00:00.000000000 +0700
+++ gcc-11.2.0.faruos/gcc/config/faruos.h	2021-12-30 12:37:31.077136067 +0700
@@ -0,0 +1,19 @@
+#undef TARGET_FARUOS
+#define TARGET_FARUOS 1
+#undef LIB_SPEC
+#define LIB_SPEC "-lkoete"
+#undef STARTFILE_SPEC
+#define STARTFILE_SPEC "crt0.o%s crti.o%s crtbegin.o%s"
+#undef ENDFILE_SPEC
+#define ENDFILE_SPEC "crtend.o%s crtn.o%s"
+#undef TARGET_OS_CPP_BUILTINS
+#define TARGET_OS_CPP_BUILTINS()      \
+  do {                                \
+    builtin_define ("__faruos__");      \
+    builtin_define ("__unix__");      \
+    builtin_assert ("system=faruos");   \
+    builtin_assert ("system=unix");   \
+    builtin_assert ("system=posix");   \
+  } while(0);
+#undef LINK_SPEC
+#define LINK_SPEC "-z max-page-size=4096"
